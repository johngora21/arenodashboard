# AI API Keys for Multi-Platform Posting System
# Copy this file to .env and replace with your actual API keys

# OpenAI API Key - For GPT-4 access
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key - For Claude access
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI API Key - For Gemini access
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Meta AI API Key - For Meta AI access
META_AI_API_KEY=your_meta_ai_api_key_here

# Microsoft Copilot API Key - For Copilot access
MICROSOFT_COPILOT_API_KEY=your_microsoft_copilot_api_key_here

# API Configuration
OPENAI_MODEL=gpt-4
ANTHROPIC_MODEL=claude-3-sonnet-20240229
GOOGLE_MODEL=gemini-pro
META_MODEL=llama-2-70b-chat
COPILOT_MODEL=copilot-gpt-4

# Rate Limiting (requests per minute)
OPENAI_RATE_LIMIT=60
ANTHROPIC_RATE_LIMIT=50
GOOGLE_RATE_LIMIT=60
META_RATE_LIMIT=30
COPILOT_RATE_LIMIT=40

# Cost Management (USD per 1000 tokens)
OPENAI_COST_PER_1K=0.03
ANTHROPIC_COST_PER_1K=0.015
GOOGLE_COST_PER_1K=0.001
META_COST_PER_1K=0.002
COPILOT_COST_PER_1K=0.02

# Environment
NODE_ENV=development

# Database (if needed for storing AI interactions)
DATABASE_URL=your_database_url_here

# Redis (for caching AI responses)
REDIS_URL=your_redis_url_here

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info

# Feature Flags
ENABLE_AI_OPTIMIZATION=true
ENABLE_COST_TRACKING=true
ENABLE_PERFORMANCE_MONITORING=true
